{
  "name": "node-cjs-interop",
  "version": "0.1.0",
  "packageManager": "yarn@3.1.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "$npm_execpath build:tsc && $npm_execpath build:babel",
    "build:babel": "babel -d dist src -x '.ts' --ignore 'src/**/*.test.ts'",
    "build:tsc": "tsc --build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@jest/globals": "^27.4.4",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "jest": "^27.4.4",
    "typescript": "^4.5.3"
  },
  "jest": {
    "extensionsToTreatAsEsm": [".ts"]
  }
}
