{
  "name": "node-cjs-interop-finder",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "$npm_execpath build:tsc && $npm_execpath build:babel",
    "build:babel": "BUILD_TARGET=esm babel -d dist src -x '.ts' --ignore 'src/**/*.test.ts'",
    "build:tsc": "tsc --build",
    "prepack": "$npm_execpath build",
    "test": "NODE_OPTIONS=--experimental-vm-modules BUILD_TARGET=esm jest"
  },
  "dependencies": {
    "@babel/parser": "^7.16.6",
    "@babel/types": "^7.16.0",
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@jest/globals": "^27.4.4",
    "@types/resolve": "^1.20.1",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "jest": "^27.4.4",
    "typescript": "^4.5.3"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts",
      ".mts"
    ]
  }
}
